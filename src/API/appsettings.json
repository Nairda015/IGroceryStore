{
  "ApplicationName": "IGroceryStore",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "Postgres": {
    "ConnectionString": "foo",
    "EnableSensitiveData": false
  },
  "AWS": {
    "Region": "foo"
  },
  "Rabbit": {
    "Host": "foo",
    "VirtualHost": "/",
    "Username": "foo",
    "Password": "foo"
  },
  "AllowedHosts": "*"
}
